{
  "name": "@ng-galien/node-red-pulsar",
  "version": "1.0.2",
  "description": "Node-RED nodes for Apache Pulsar",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ng-galien/node-red-contrib-pulsar.git"
  },
  "publishConfig": {
    "@ng-galien:registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "format": "eslint --max-warnings=0 --fix src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "deploy": "PROJECT_DIR=$(pwd) && cd ~/.node-red && npm install $PROJECT_DIR",
    "test": "mocha test/*.spec.js"
  },
  "node-red": {
    "nodes": {
      "pulsar-config": "src/pulsar-config.js",
      "pulsar-consumer": "src/pulsar-consumer.js",
      "pulsar-producer": "src/pulsar-producer.js",
      "pulsar-schema": "src/pulsar-schema.js"
    }
  },
  "keywords": [
    "node-red"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "avsc": "^5.7.7",
    "pulsar-client": "^1.11.0",
    "save-dev": "^0.0.1-security",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node-red": "^1.3.5",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "axios": "^1.6.3",
    "chai": "^5.1.0",
    "chai-wait-for": "^2.0.2",
    "eslint": "^8.0.1",
    "mocha": "^10.2.0",
    "node-red": "^3.1.3",
    "node-red-node-test-helper": "^0.3.3",
    "testcontainers": "^10.4.0",
    "typescript": "^5.4.5"
  }
}
